{
  "name": "@golemcloud/golem-ts-sdk-tests",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "clean-test-data": "rm -rf .metadata",
    "generate-sample-agents-types": "npx golem-typegen ./tsconfig.json --files tests/**/*.ts --include-class-decorators agent",
    "prebuild": "npm run clean-test-data",
    "build": "pnpm run prebuild && tsc --noEmit",
    "clean": "rm -rf .metadata agent-template dist node_modules package-lock.json && printf '\\nRun `npm install` before building again.\\n\\n'",
    "test-typegen": "vitest -c ./typegen-tests.vitest.config.ts run",
    "test": "npm run build && tsc --noEmit && npm run test-typegen",
    "dev": "rollup -c -w"
  },
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@golemcloud/golem-ts-typegen": "workspace:*",
    "@golemcloud/golem-ts-sdk": "workspace:*",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "fast-check": "^4.2.0",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.1.14",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-morph": "^26.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "repository": "https://github.com/golemcloud/golem"
}
