{
  "name": "golem-agentic",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm --filter @golemcloud/golem-ts-types-core run build && pnpm --filter @golemcloud/golem-ts-typegen run build && pnpm --filter @golemcloud/golem-ts-sdk run build",
    "build-agent-template": "pnpm --filter @golemcloud/golem-ts-sdk run build-agent-template",
    "lint": "pnpm -r run lint",
    "test": "pnpm -r run test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist node_modules packages/*/dist packages/*/node_modules packages/*/.metadata packages/golem-ts-sdk/agent-template"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "pnpm": "^8.10.0",
    "prettier": "^3.6.2",
    "rollup": "^4.46.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a"
}
